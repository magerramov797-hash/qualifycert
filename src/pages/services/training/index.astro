---
import BaseLayout from "../../../layouts/BaseLayout.astro";
const title = "Обучение";
const desc = "Обучение по ISO: внутренний аудитор, внедрение, риски, CAPA, HACCP.";
---

<BaseLayout title={title} description={desc}>
  <section class="section">
    <div class="container">
      <h1 class="h1">{title}</h1>
      <p class="p">Обучаем сотрудников и аудиторов: теория + практика на реальных примерах, шаблоны и чек-листы.</p>
    </div>
  </section>
</BaseLayout>
